{
  "buildCommand": "echo 'No build needed - static site'",
  "outputDirectory": "frontend",
  "devCommand": "python server.py",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, must-revalidate"
        }
      ]
    }
  ]
}

